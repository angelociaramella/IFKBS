FUNCTION_BLOCK effetti_allergenici_popolazione


VAR_INPUT
   rischio_polline : REAL;
   allergenicita_popolazione : REAL;
END_VAR

VAR_OUTPUT
   effetti_allergenici_popolazione : REAL;
END_VAR

FUZZIFY rischio_polline
   TERM alto := 100 ;
   TERM basso := 0 ;
   TERM medio := 50 ;
END_FUZZIFY
FUZZIFY allergenicita_popolazione
   TERM alto := 100 ;
